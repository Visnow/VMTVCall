<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:enterFadeDuration="100"
    android:exitFadeDuration="100">
    <item android:dither="true" android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/vm_dimen_16"/>
            <solid android:color="@color/vm_green_54"/>
        </shape>
    </item>
    <!--获取焦点-->
    <item android:dither="true" android:state_focused="true">
        <shape>
            <corners android:radius="@dimen/vm_dimen_16"/>
            <solid android:color="@color/vm_green_87"/>
        </shape>
    </item>
    <item>
        <shape android:dither="true">
            <corners android:radius="@dimen/vm_dimen_16"/>
            <solid android:color="@color/vm_white_100"/>
        </shape>
    </item>
</selector>